package test2;


public class average {
	public static void main(String[] args) {

	int a[] = {9, 8, 7, 6,5,4,3,2,1};
	int sum = 0;
	int avg = 0;
	int count = 0;
	
	for(int i = 0; i < a.length; i++) {
		sum = sum + a[i];
		count++;
	}
	avg = sum / count;
	System.out.println("Average value is: " + avg);

}
}
